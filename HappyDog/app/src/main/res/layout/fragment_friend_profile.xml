<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dee2e6"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.FriendProfileFragment">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="409dp"
        android:layout_height="632dp"
        android:src="@drawable/row"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/cancel"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.138"></ImageView>

    <ImageView
        android:id="@+id/profile_imageview"
        android:layout_width="191dp"
        android:layout_height="186dp"
        android:layout_marginTop="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_textview_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textColor="#505050"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_imageview" />


    <TextView
        android:id="@+id/profile_textview_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:hint="이름"
        android:inputType="textPersonName"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_textview_email" />


    <TextView
        android:id="@+id/profile_contract"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="지금까지"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_textview_name"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/count"
        android:layout_width="43dp"
        android:layout_height="38dp"
        android:layout_marginTop="35dp"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="0"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.388"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_textview_name"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/profile_contract2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="번 계약을 맺었습니다!"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.819"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_textview_name"
        app:layout_constraintVertical_bias="0.003" />

    <ImageView
        android:id="@+id/requestContract"
        android:layout_width="148dp"
        android:layout_height="50dp"
        android:layout_marginBottom="116dp"
        android:src="@drawable/petseat"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.11"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_contract"
        app:layout_constraintVertical_bias="1.0" />



    <ImageView
        android:id="@+id/sendMessage"
        android:layout_width="148dp"
        android:layout_height="50dp"
        android:layout_marginBottom="116dp"
        android:src="@drawable/petchating"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.889"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_contract"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bb"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:hint="펫시팅 요청"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/requestContract"
        app:layout_constraintEnd_toEndOf="@+id/requestContract"
        app:layout_constraintStart_toStartOf="@+id/requestContract"
        app:layout_constraintTop_toTopOf="@+id/requestContract"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/cc"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:hint="대화 요청"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/sendMessage"
        app:layout_constraintEnd_toEndOf="@+id/sendMessage"
        app:layout_constraintStart_toStartOf="@+id/sendMessage"
        app:layout_constraintTop_toTopOf="@+id/sendMessage" />

</androidx.constraintlayout.widget.ConstraintLayout>




